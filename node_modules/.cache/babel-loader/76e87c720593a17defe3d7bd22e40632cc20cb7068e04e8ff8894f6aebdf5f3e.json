         </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\n\nexport default {\n  name: \"PostComponent\",\n  props: {\n    id: Number,\n  },\n  data() {\n    return {\n      showModal: false,\n    };\n  },\n  computed: {\n    ...mapGetters(\"post\", [\"allpost\"]),\n  },\n  methods: {\n    ...mapActions(\"post\", [\"getPost\"]),\n    details(id) {\n      console.log(id);\n      this.showModal = true;\n    },\n  },\n  created() {\n    this.$store.dispatch(\"post/getPost\");\n  },\n};\n</script>\n"],"mappings":"AAwFA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,EAAE,EAAEC;EACN,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGP,UAAU,CAAC,MAAM,EAAE,CAAC,SAAS,CAAC;EACnC,CAAC;EACDQ,OAAO,EAAE;IACP,GAAGT,UAAU,CAAC,MAAM,EAAE,CAAC,SAAS,CAAC,CAAC;IAClCU,OAAOA,CAACN,EAAE,EAAE;MACVO,OAAO,CAACC,GAAG,CAACR,EAAE,CAAC;MACf,IAAI,CAACG,SAAQ,GAAI,IAAI;IACvB;EACF,CAAC;EACDM,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,cAAc,CAAC;EACtC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}